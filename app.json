{
  "expo": {
    "name": "MyDiary",
    "slug": "diary",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "INTERNET",
        "CAMERA",
        "ACCESS_FINE_LOCATION"
      ],
      "package": "com.motyaozzz.diary"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "extra": {
      "eas": {
        "projectId": "c414bdc7-f3c8-4668-9ce8-76196e5d0077"
      },
      "router": {
        "origin": false
      }
    },
    "plugins": [
      "expo-router",
      // [
      //   "expo-splash-screen",
      //   {
      //    "image": "./assets/images/diary-main.png",
      //     "imageWidth": 200,
      //     "resizeMode": "contain",
      //     "backgroundColor": "#000000"
      //   }
      // ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "motyaozzz"
  }
}
